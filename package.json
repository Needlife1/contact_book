{
  "name": "contact_book",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint:editor": "editorconfig-checker",
    "lint:fs": "ls-lint",
    "lint:js": "npx tsc --noEmit && npx eslint \"**/*.{ts,tsx}\" --max-warnings=0",
    "lint:css": "npx stylelint \"src/**/*.scss\"",
    "lint:prettify": "prettier --check \"**/*.{ts,tsx,json,md,scss,html}\"",
    "lint": "npm run lint:editor && npm run lint:fs && npm run lint:js && npm run lint:css && npm run lint:prettify",
    "fix:css": "npx stylelint --fix \"src/**/*.scss\"",
    "prettify": "prettier --write \"**/*.{ts,tsx,json,md,scss,html}\"",
    "start:dev": "vite",
    "build": "tsc --build --clean && vite build",
    "preview": "vite preview"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^2.2.3",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "editorconfig-checker": "^5.1.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^52.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.74.1",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.3.1",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-scss": "^6.2.1",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
